{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["import"],
    "env": {
        "es6": true,
        "jest": true,
        "browser": true,
        "module": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "sourceType": "module",
        "allowImportExportEverywhere": true
    }
}